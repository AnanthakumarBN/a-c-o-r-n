<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:j2seproject3="http://www.netbeans.org/ns/j2se-project/3" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-AcornWSService" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/AcornWSService"/>
        <property name="wsdl-AcornWSService" location="xml-resources/web-service-references/AcornWSService/wsdl/sysbio3.fhms.surrey.ac.uk_8080/acorn/AcornWSService.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/AcornWSService" destdir="${build.generated.dir}/jax-wsCache/AcornWSService" wsdl="${wsdl-AcornWSService}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://sysbio3.fhms.surrey.ac.uk:8080/acorn/AcornWSService?wsdl" xnocompile="true">
            <depends file="${wsdl-AcornWSService}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/AcornWSService"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/AcornWSService">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-AcornWSService" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/AcornWSService"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/acorn/webservice"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-AcornWSService"/>
</project>
